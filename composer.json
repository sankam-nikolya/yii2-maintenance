{
    "name": "sankam/yii2-maintenance",
    "description": "Maintenance Component for Yii2",
    "keywords": ["yii", "maintenance"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/sankam-nikolya/yii2-maintenance"
    },
    "authors": [
        {
            "name": "Nikolya",
            "email": "k_m_i@i.ua"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "sankam\\maintenance\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
